{
  "name": "pubcore-react-input-suggest",
  "version": "1.0.0",
  "description": "text input field with selectable suggestions for react",
  "main": "js/InputSuggest.js",
  "scripts": {
    "test": "mocha --require @babel/register --require ./test/.helpers.js --require ./test/.dom.js --recursive",
    "build": "babel src --out-dir js --ignore spec.js",
    "watch": "watch \"npm run build\" ./src",
    "pack": "npm run build && webpack --optimize-minimize",
    "prepare": "npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubcore/reactInputSuggest.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pubcore/reactInputSuggest/issues"
  },
  "homepage": "https://github.com/pubcore/reactInputSuggest#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "pubcore-ui-text": "^1.0.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-onclickoutside": "^6.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-adapter-utils": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "watch": "^1.0.2",
    "webpack": "^4.33.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
